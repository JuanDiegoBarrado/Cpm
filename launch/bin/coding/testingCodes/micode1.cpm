clas Chavalito {
    int fafa;

    Chavalito(int x) {
        this.fafa = x;
    }

    public func getFafa() : int {
        return fafa;
    }
}

clas Alumno {
    praivet int nota;
    public int notilla;
    praivet Chavalito pibe;

    Alumno(int a, int b) {
        this.nota = a;
        this.notilla = b;
        this.pibe = Chavalito(4);
    }

    public func consultaFafa() : int {
        int fafa = pibe.getFafa();
        return fafa;
    }
}

func mein() : int {
    Alumno a = Alumno(2, 3);
    int fafilla = a.getFafa();
    ceaut(fafilla);
    return 0;
}

