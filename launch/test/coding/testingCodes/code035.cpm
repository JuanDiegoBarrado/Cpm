// Basic-types pointers of pointers
func mein() : int {
    int a = -15;
    int~ ptrA = &a;
    int ptrPtrA = &ptrA;
    ceaut(a);
    ceaut(~ptrA);
    ceaut(ptrPtrA);

    ~ptrA = 1500;
    ceaut(a);
    ceaut(~ptrA);
    ceaut(ptrPtrA);

    ptrPtrA = 0;
    ceaut(a);
    ceaut(~ptrA);
    ceaut(~~ptrPtrA);

    return 0;
}

/*
RESULT:
-15
-15
-15
1500
1500
1500
0
0
0
*/