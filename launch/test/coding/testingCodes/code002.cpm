    for (int i = 0; i < 10; i = i + 1) {
        vector[i] = (i * 3) + (i / 2);
    }
    vector[10 / 2] = 1000;

    int maximum = 0;
    for (int i = 0; i < 10; i = i + 1) {
        maximum = max(maximum, vector[i]);
    }
    
    ceaut(maximum);
    return 0;
}

