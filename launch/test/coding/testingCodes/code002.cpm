func max(int a, int b) : bul {
    return a < b;
}

difain MAXI 10;

func mein() : int {
    int vector[MAXI];

    for (int i = 0; i < MAXI; i++) {
        vector[i] = (i * 3) + (i / 2);
    }
    vector[MAXI / 2] = 1000;

    int maximum = 0;
    for (int i = 0; i < MAXI; i++) {
        maximum = max(maximum, vector[i]);
    }
    
    ceaut(maximum);
    return 0;
}

